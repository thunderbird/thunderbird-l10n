search-button =
    .label = ਖੋਜ
    .accesskey = S
stop-button =
    .label = ਰੋਕੋ
    .accesskey = S
# Variables:
# $count (Number) - Number of matches found.
matches-found =
    .value =
        { $count ->
            [one] { $count } match found
           *[other] { $count } matches found
        }
no-matches-found =
    .value = ਕੋਈ ਮੇਲ ਨਹੀਂ ਲੱਭਿਆ
