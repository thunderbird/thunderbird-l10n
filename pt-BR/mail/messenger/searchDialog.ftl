search-button =
    .label = Pesquisar
    .accesskey = P
stop-button =
    .label = Parar
    .accesskey = P
searching-message =
    .value = Pesquisando…
# Variables:
# $count (Number) - Number of matches found.
matches-found =
    .value =
        { $count ->
            [one] { $count } item encontrado
           *[other] { $count } itens encontrados
        }
no-matches-found =
    .value = Nenhum item encontrado
search-heading =
    .value = Procurar mensagens em:
    .accesskey = z
search-subfolders =
