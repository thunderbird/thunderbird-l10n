searching-message =
    .value = Se caută…
# Variables:
# $count (Number) - Number of matches found.
matches-found =
    .value =
        { $count ->
            [one] { $count } rezultat găsit
            [few] { $count } rezultate găsite
           *[other] { $count } rezultate găsite
        }
no-matches-found =
    .value = Niciun rezultat
