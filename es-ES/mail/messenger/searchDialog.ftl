search-button =
    .label = Buscar
    .accesskey = S
stop-button =
    .label = Detener
    .accesskey = D
searching-message =
    .value = Buscando…
# Variables:
# $count (Number) - Number of matches found.
matches-found =
    .value =
        { $count ->
            [one] { $count } coincidencia encontrada
           *[other] { $count } coincidencias encontradas
        }
no-matches-found =
    .value = No se han encontrado coincidencias
